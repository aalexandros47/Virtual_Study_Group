{"ast":null,"code":"import { db } from '../firebase';\nexport default {\n  name: 'ResourceShare',\n  props: {\n    resources: Array\n  },\n  data() {\n    return {\n      newResource: {\n        name: '',\n        url: ''\n      }\n    };\n  },\n  methods: {\n    async addResource() {\n      try {\n        const resourceRef = await db.collection('studyGroups').doc(this.$route.params.id).collection('resources').add(this.newResource);\n        this.newResource = {\n          name: '',\n          url: ''\n        };\n        this.$emit('resource-added', resourceRef.id);\n      } catch (error) {\n        console.error('Error adding resource:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["db","name","props","resources","Array","data","newResource","url","methods","addResource","resourceRef","collection","doc","$route","params","id","add","$emit","error","console"],"sources":["/Users/ag47/virtual-study-group-platfor copy/src/components/ResourceShare.vue"],"sourcesContent":["<template>\n  <div class=\"resource-share\">\n    <h3>Shared Resources</h3>\n    <ul>\n      <li v-for=\"resource in resources\" :key=\"resource.id\">\n        <a :href=\"resource.url\" target=\"_blank\">{{ resource.name }}</a>\n      </li>\n    </ul>\n    <form @submit.prevent=\"addResource\">\n      <div>\n        <label for=\"name\">Resource Name:</label>\n        <input type=\"text\" id=\"name\" v-model=\"newResource.name\" required />\n      </div>\n      <div>\n        <label for=\"url\">Resource URL:</label>\n        <input type=\"url\" id=\"url\" v-model=\"newResource.url\" required />\n      </div>\n      <button type=\"submit\">Add Resource</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { db } from '../firebase';\n\nexport default {\n  name: 'ResourceShare',\n  props: {\n    resources: Array,\n  },\n  data() {\n    return {\n      newResource: {\n        name: '',\n        url: '',\n      },\n    };\n  },\n  methods: {\n    async addResource() {\n      try {\n        const resourceRef = await db\n          .collection('studyGroups')\n          .doc(this.$route.params.id)\n          .collection('resources')\n          .add(this.newResource);\n        this.newResource = { name: '', url: '' };\n        this.$emit('resource-added', resourceRef.id);\n      } catch (error) {\n        console.error('Error adding resource:', error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.resource-share {\n  padding: 2em;\n}\n</style>\n"],"mappings":"AAuBA,SAASA,EAAC,QAAS,aAAa;AAEhC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,SAAS,EAAEC;EACb,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;QACXL,IAAI,EAAE,EAAE;QACRM,GAAG,EAAE;MACP;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAMC,WAAU,GAAI,MAAMV,EAAC,CACxBW,UAAU,CAAC,aAAa,EACxBC,GAAG,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,EACzBJ,UAAU,CAAC,WAAW,EACtBK,GAAG,CAAC,IAAI,CAACV,WAAW,CAAC;QACxB,IAAI,CAACA,WAAU,GAAI;UAAEL,IAAI,EAAE,EAAE;UAAEM,GAAG,EAAE;QAAG,CAAC;QACxC,IAAI,CAACU,KAAK,CAAC,gBAAgB,EAAEP,WAAW,CAACK,EAAE,CAAC;MAC9C,EAAE,OAAOG,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}