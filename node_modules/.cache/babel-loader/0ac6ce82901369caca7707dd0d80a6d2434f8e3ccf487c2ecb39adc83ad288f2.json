p',\n  components: {\n    SearchFilter, // Ensure the component is registered here\n  },\n  computed: {\n    filteredStudyGroups() {\n      return this.$store.state.filteredStudyGroups;\n    },\n    authMessage() {\n      return this.$store.state.authMessage;\n    },\n    authError() {\n      return this.$store.state.authError;\n    },\n  },\n  methods: {\n    joinGroup(groupId) {\n      this.$store.dispatch('joinStudyGroup', groupId);\n    },\n    likeGroup(groupId) {\n      this.$store.dispatch('likeStudyGroup', groupId);\n    },\n  },\n  created() {\n    this.$store.commit('setAuthMessage', null);\n    this.$store.commit('setAuthError', null);\n    this.$store.dispatch('fetchStudyGroups');\n  },\n};\n</script>\n\n<style scoped>\n.join-study-group {\n  padding: 2em;\n}\n.study-group {\n  margin-bottom: 1em;\n  padding: 1em;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n.auth-message {\n  color: green;\n  margin-top: 1em;\n}\n.auth-error {\n  color: red;\n  margin-top: 1em;\n}\n</style>\n"],"mappings":"AAuBA,OAAOA,YAAW,MAAO,oBAAoB,EAAE;;AAE/C,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVF,YAAY,CAAE;EAChB,CAAC;EACDG,QAAQ,EAAE;IACRC,mBAAmBA,CAAA,EAAG;MACpB,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,mBAAmB;IAC9C,CAAC;IACDG,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,WAAW;IACtC,CAAC;IACDC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACH,MAAM,CAACC,KAAK,CAACE,SAAS;IACpC;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,OAAO,EAAE;MACjB,IAAI,CAACN,MAAM,CAACO,QAAQ,CAAC,gBAAgB,EAAED,OAAO,CAAC;IACjD,CAAC;IACDE,SAASA,CAACF,OAAO,EAAE;MACjB,IAAI,CAACN,MAAM,CAACO,QAAQ,CAAC,gBAAgB,EAAED,OAAO,CAAC;IACjD;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACT,MAAM,CAACU,MAAM,CAAC,gBAAgB,EAAE,IAAI,CAAC;IAC1C,IAAI,CAACV,MAAM,CAACU,MAAM,CAAC,cAAc,EAAE,IAAI,CAAC;IACxC,IAAI,CAACV,MAAM,CAACO,QAAQ,CAAC,kBAAkB,CAAC;EAC1C;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}