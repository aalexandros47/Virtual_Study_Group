{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport DiscussionBoard from '../components/DiscussionBoard.vue';\nimport ResourceShare from '../components/ResourceShare.vue';\nimport { db } from '../firebase';\nexport default {\n  name: 'StudyGroup',\n  components: {\n    DiscussionBoard,\n    ResourceShare\n  },\n  data() {\n    return {\n      groupId: this.$route.params.id,\n      studyGroup: {},\n      selectedTab: 'video',\n      videoStream: null,\n      audioStream: null,\n      videoStreams: [] // Store multiple video streams\n    };\n  },\n  methods: {\n    selectTab(tab) {\n      this.selectedTab = tab;\n    },\n    async toggleCamera() {\n      if (!this.videoStream) {\n        try {\n          this.videoStream = await navigator.mediaDevices.getUserMedia({\n            video: true\n          });\n          this.videoStreams.push(this.videoStream);\n          this.updateVideoGrid();\n        } catch (error) {\n          console.error('Error accessing camera:', error);\n        }\n      } else {\n        this.videoStreams.forEach(stream => stream.getTracks().forEach(track => track.stop()));\n        this.videoStreams = [];\n      }\n    },\n    async toggleMicrophone() {\n      if (!this.audioStream) {\n        try {\n          this.audioStream = await navigator.mediaDevices.getUserMedia({\n            audio: true\n          });\n          const audioElement = document.getElementById('audio');\n          audioElement.srcObject = this.audioStream;\n        } catch (error) {\n          console.error('Error accessing microphone:', error);\n        }\n      } else {\n        this.audioStream.getTracks().forEach(track => track.stop());\n        this.audioStream = null;\n      }\n    },\n    updateVideoGrid() {\n      this.$nextTick(() => {\n        this.videoStreams.forEach((stream, index) => {\n          const videoElement = document.getElementById('video-' + index);\n          if (videoElement) {\n            videoElement.srcObject = stream;\n          }\n        });\n      });\n    },\n    async fetchStudyGroup() {\n      try {\n        const groupDoc = await db.collection('studyGroups').doc(this.groupId).get();\n        this.studyGroup = groupDoc.data();\n      } catch (error) {\n        console.error('Error fetching study group:', error);\n      }\n    }\n  },\n  created() {\n    this.fetchStudyGroup();\n  }\n};","map":{"version":3,"names":["DiscussionBoard","ResourceShare","db","name","components","data","groupId","$route","params","id","studyGroup","selectedTab","videoStream","audioStream","videoStreams","methods","selectTab","tab","toggleCamera","navigator","mediaDevices","getUserMedia","video","push","updateVideoGrid","error","console","forEach","stream","getTracks","track","stop","toggleMicrophone","audio","audioElement","document","getElementById","srcObject","$nextTick","index","videoElement","fetchStudyGroup","groupDoc","collection","doc","get","created"],"sources":["/Users/ag47/virtual-study-group-platfor Design 2/src/views/StudyGroup.vue"],"sourcesContent":["<template>\n  <div\n    class=\"study-group-page bg-white shadow-lg rounded-lg p-8 m-4 max-w-4xl mx-auto\"\n  >\n    <h2 class=\"text-3xl font-bold mb-4\">{{ studyGroup.name }}</h2>\n    <div class=\"tabs\">\n      <button\n        @click=\"selectTab('video')\"\n        :class=\"{ 'active-tab': selectedTab === 'video' }\"\n      >\n        Video\n      </button>\n      <button\n        @click=\"selectTab('discussion')\"\n        :class=\"{ 'active-tab': selectedTab === 'discussion' }\"\n      >\n        Discussion\n      </button>\n      <button\n        @click=\"selectTab('resources')\"\n        :class=\"{ 'active-tab': selectedTab === 'resources' }\"\n      >\n        Resources\n      </button>\n    </div>\n    <div v-if=\"selectedTab === 'video'\" class=\"video-container mb-4\">\n      <div class=\"video-grid\">\n        <video\n          v-for=\"(stream, index) in videoStreams\"\n          :key=\"index\"\n          :id=\"'video-' + index\"\n          class=\"video-element\"\n          autoplay\n          playsinline\n        ></video>\n      </div>\n      <button\n        @click=\"toggleCamera\"\n        class=\"bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded-full transition duration-300\"\n      >\n        Toggle Camera\n      </button>\n      <button\n        @click=\"toggleMicrophone\"\n        class=\"ml-4 bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded-full transition duration-300\"\n      >\n        Toggle Microphone\n      </button>\n    </div>\n    <div v-if=\"selectedTab === 'discussion'\" class=\"discussion-container\">\n      <DiscussionBoard :studyGroupId=\"groupId\" />\n    </div>\n    <div v-if=\"selectedTab === 'resources'\" class=\"resources-container\">\n      <ResourceShare :studyGroupId=\"groupId\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport DiscussionBoard from '../components/DiscussionBoard.vue';\nimport ResourceShare from '../components/ResourceShare.vue';\nimport { db } from '../firebase';\n\nexport default {\n  name: 'StudyGroup',\n  components: {\n    DiscussionBoard,\n    ResourceShare,\n  },\n  data() {\n    return {\n      groupId: this.$route.params.id,\n      studyGroup: {},\n      selectedTab: 'video',\n      videoStream: null,\n      audioStream: null,\n      videoStreams: [], // Store multiple video streams\n    };\n  },\n  methods: {\n    selectTab(tab) {\n      this.selectedTab = tab;\n    },\n    async toggleCamera() {\n      if (!this.videoStream) {\n        try {\n          this.videoStream = await navigator.mediaDevices.getUserMedia({\n            video: true,\n          });\n          this.videoStreams.push(this.videoStream);\n          this.updateVideoGrid();\n        } catch (error) {\n          console.error('Error accessing camera:', error);\n        }\n      } else {\n        this.videoStreams.forEach((stream) =>\n          stream.getTracks().forEach((track) => track.stop())\n        );\n        this.videoStreams = [];\n      }\n    },\n    async toggleMicrophone() {\n      if (!this.audioStream) {\n        try {\n          this.audioStream = await navigator.mediaDevices.getUserMedia({\n            audio: true,\n          });\n          const audioElement = document.getElementById('audio');\n          audioElement.srcObject = this.audioStream;\n        } catch (error) {\n          console.error('Error accessing microphone:', error);\n        }\n      } else {\n        this.audioStream.getTracks().forEach((track) => track.stop());\n        this.audioStream = null;\n      }\n    },\n    updateVideoGrid() {\n      this.$nextTick(() => {\n        this.videoStreams.forEach((stream, index) => {\n          const videoElement = document.getElementById('video-' + index);\n          if (videoElement) {\n            videoElement.srcObject = stream;\n          }\n        });\n      });\n    },\n    async fetchStudyGroup() {\n      try {\n        const groupDoc = await db\n          .collection('studyGroups')\n          .doc(this.groupId)\n          .get();\n        this.studyGroup = groupDoc.data();\n      } catch (error) {\n        console.error('Error fetching study group:', error);\n      }\n    },\n  },\n  created() {\n    this.fetchStudyGroup();\n  },\n};\n</script>\n\n<style scoped>\n.study-group-page {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 2em;\n}\n\n.tabs {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 1em;\n}\n\n.tabs button {\n  margin: 0 1em;\n  padding: 0.5em 1em;\n  font-weight: bold;\n  cursor: pointer;\n}\n\n.active-tab {\n  background-color: #007bff;\n  color: white;\n  border-radius: 5px;\n}\n\n.video-container {\n  width: 100%;\n  max-width: 600px;\n  margin-bottom: 1em;\n  position: relative;\n}\n\n.video-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\n  gap: 10px;\n}\n\nvideo {\n  width: 100%;\n  border-radius: 8px;\n}\n\n.controls {\n  margin-bottom: 1em;\n}\n\n.content {\n  display: flex;\n  justify-content: space-between;\n  width: 100%;\n  max-width: 1200px;\n}\n\n.discussion-board,\n.resource-share {\n  flex: 1;\n  margin: 0 1em;\n}\n</style>\n"],"mappings":";AA2DA,OAAOA,eAAc,MAAO,mCAAmC;AAC/D,OAAOC,aAAY,MAAO,iCAAiC;AAC3D,SAASC,EAAC,QAAS,aAAa;AAEhC,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVJ,eAAe;IACfC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;MAC9BC,UAAU,EAAE,CAAC,CAAC;MACdC,WAAW,EAAE,OAAO;MACpBC,WAAW,EAAE,IAAI;MACjBC,WAAW,EAAE,IAAI;MACjBC,YAAY,EAAE,EAAE,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,GAAG,EAAE;MACb,IAAI,CAACN,WAAU,GAAIM,GAAG;IACxB,CAAC;IACD,MAAMC,YAAYA,CAAA,EAAG;MACnB,IAAI,CAAC,IAAI,CAACN,WAAW,EAAE;QACrB,IAAI;UACF,IAAI,CAACA,WAAU,GAAI,MAAMO,SAAS,CAACC,YAAY,CAACC,YAAY,CAAC;YAC3DC,KAAK,EAAE;UACT,CAAC,CAAC;UACF,IAAI,CAACR,YAAY,CAACS,IAAI,CAAC,IAAI,CAACX,WAAW,CAAC;UACxC,IAAI,CAACY,eAAe,CAAC,CAAC;QACxB,EAAE,OAAOC,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;QACjD;MACF,OAAO;QACL,IAAI,CAACX,YAAY,CAACa,OAAO,CAAEC,MAAM,IAC/BA,MAAM,CAACC,SAAS,CAAC,CAAC,CAACF,OAAO,CAAEG,KAAK,IAAKA,KAAK,CAACC,IAAI,CAAC,CAAC,CACpD,CAAC;QACD,IAAI,CAACjB,YAAW,GAAI,EAAE;MACxB;IACF,CAAC;IACD,MAAMkB,gBAAgBA,CAAA,EAAG;MACvB,IAAI,CAAC,IAAI,CAACnB,WAAW,EAAE;QACrB,IAAI;UACF,IAAI,CAACA,WAAU,GAAI,MAAMM,SAAS,CAACC,YAAY,CAACC,YAAY,CAAC;YAC3DY,KAAK,EAAE;UACT,CAAC,CAAC;UACF,MAAMC,YAAW,GAAIC,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC;UACrDF,YAAY,CAACG,SAAQ,GAAI,IAAI,CAACxB,WAAW;QAC3C,EAAE,OAAOY,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;QACrD;MACF,OAAO;QACL,IAAI,CAACZ,WAAW,CAACgB,SAAS,CAAC,CAAC,CAACF,OAAO,CAAEG,KAAK,IAAKA,KAAK,CAACC,IAAI,CAAC,CAAC,CAAC;QAC7D,IAAI,CAAClB,WAAU,GAAI,IAAI;MACzB;IACF,CAAC;IACDW,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACc,SAAS,CAAC,MAAM;QACnB,IAAI,CAACxB,YAAY,CAACa,OAAO,CAAC,CAACC,MAAM,EAAEW,KAAK,KAAK;UAC3C,MAAMC,YAAW,GAAIL,QAAQ,CAACC,cAAc,CAAC,QAAO,GAAIG,KAAK,CAAC;UAC9D,IAAIC,YAAY,EAAE;YAChBA,YAAY,CAACH,SAAQ,GAAIT,MAAM;UA