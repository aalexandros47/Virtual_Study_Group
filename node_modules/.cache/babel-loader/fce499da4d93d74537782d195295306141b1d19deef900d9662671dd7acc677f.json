{"ast":null,"code":"import { db } from '../firebase';\nexport default {\n  name: 'SessionScheduler',\n  props: {\n    sessions: Array\n  },\n  data() {\n    return {\n      newSession: {\n        topic: '',\n        date: ''\n      }\n    };\n  },\n  methods: {\n    async addSession() {\n      try {\n        const sessionRef = await db.collection('studyGroups').doc(this.$route.params.id).collection('sessions').add(this.newSession);\n        this.newSession = {\n          topic: '',\n          date: ''\n        };\n        this.$emit('session-added', sessionRef.id);\n      } catch (error) {\n        console.error('Error adding session:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["db","name","props","sessions","Array","data","newSession","topic","date","methods","addSession","sessionRef","collection","doc","$route","params","id","add","$emit","error","console"],"sources":["/Users/ag47/virtual-study-group-platfor copy/src/components/SessionScheduler.vue"],"sourcesContent":["<template>\n  <div class=\"session-scheduler\">\n    <h3>Upcoming Sessions</h3>\n    <ul>\n      <li v-for=\"session in sessions\" :key=\"session.id\">\n        {{ session.date }} - {{ session.topic }}\n      </li>\n    </ul>\n    <form @submit.prevent=\"addSession\">\n      <div>\n        <label for=\"topic\">Topic:</label>\n        <input type=\"text\" id=\"topic\" v-model=\"newSession.topic\" required />\n      </div>\n      <div>\n        <label for=\"date\">Date:</label>\n        <input type=\"date\" id=\"date\" v-model=\"newSession.date\" required />\n      </div>\n      <button type=\"submit\">Add Session</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport { db } from '../firebase';\n\nexport default {\n  name: 'SessionScheduler',\n  props: {\n    sessions: Array,\n  },\n  data() {\n    return {\n      newSession: {\n        topic: '',\n        date: '',\n      },\n    };\n  },\n  methods: {\n    async addSession() {\n      try {\n        const sessionRef = await db\n          .collection('studyGroups')\n          .doc(this.$route.params.id)\n          .collection('sessions')\n          .add(this.newSession);\n        this.newSession = { topic: '', date: '' };\n        this.$emit('session-added', sessionRef.id);\n      } catch (error) {\n        console.error('Error adding session:', error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.session-scheduler {\n  padding: 2em;\n}\n</style>\n"],"mappings":"AAuBA,SAASA,EAAC,QAAS,aAAa;AAEhC,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE;IACLC,QAAQ,EAAEC;EACZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;QACVC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE;MACR;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,UAAS,GAAI,MAAMX,EAAC,CACvBY,UAAU,CAAC,aAAa,EACxBC,GAAG,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,EACzBJ,UAAU,CAAC,UAAU,EACrBK,GAAG,CAAC,IAAI,CAACX,UAAU,CAAC;QACvB,IAAI,CAACA,UAAS,GAAI;UAAEC,KAAK,EAAE,EAAE;UAAEC,IAAI,EAAE;QAAG,CAAC;QACzC,IAAI,CAACU,KAAK,CAAC,eAAe,EAAEP,UAAU,CAACK,EAAE,CAAC;MAC5C,EAAE,OAAOG,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;MAC/C;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}