{"ast":null,"code":"import { db } from '../firebase';\nexport default {\n  name: 'ResourceShare',\n  data() {\n    return {\n      resourceName: '',\n      resourceUrl: '',\n      resources: []\n    };\n  },\n  methods: {\n    async addResource() {\n      if (!this.resourceName || !this.resourceUrl) return;\n      const newResource = {\n        name: this.resourceName,\n        url: this.resourceUrl,\n        createdAt: new Date()\n      };\n      await db.collection('studyGroups').doc(this.$route.params.id).collection('resources').add(newResource);\n      this.resourceName = '';\n      this.resourceUrl = '';\n      this.fetchResources();\n    },\n    async fetchResources() {\n      const snapshot = await db.collection('studyGroups').doc(this.$route.params.id).collection('resources').orderBy('createdAt').get();\n      this.resources = snapshot.docs.map(doc => ({\n        id: doc.id,\n        ...doc.data()\n      }));\n    }\n  },\n  created() {\n    this.fetchResources();\n  }\n};","map":{"version":3,"names":["db","name","data","resourceName","resourceUrl","resources","methods","addResource","newResource","url","createdAt","Date","collection","doc","$route","params","id","add","fetchResources","snapshot","orderBy","get","docs","map","created"],"sources":["/Users/ag47/virtual-study-group-platfor copy/src/components/ResourceShare.vue"],"sourcesContent":["<template>\n  <div class=\"resource-share\">\n    <h2>Resource Sharing</h2>\n    <form @submit.prevent=\"addResource\">\n      <div>\n        <label for=\"name\">Resource Name:</label>\n        <input type=\"text\" id=\"name\" v-model=\"resourceName\" required />\n      </div>\n      <div>\n        <label for=\"url\">Resource URL:</label>\n        <input type=\"url\" id=\"url\" v-model=\"resourceUrl\" required />\n      </div>\n      <button type=\"submit\">Add Resource</button>\n    </form>\n    <div v-if=\"resources.length > 0\">\n      <h3>Resources</h3>\n      <div v-for=\"resource in resources\" :key=\"resource.id\" class=\"resource\">\n        <a :href=\"resource.url\" target=\"_blank\">{{ resource.name }}</a>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { db } from '../firebase';\n\nexport default {\n  name: 'ResourceShare',\n  data() {\n    return {\n      resourceName: '',\n      resourceUrl: '',\n      resources: [],\n    };\n  },\n  methods: {\n    async addResource() {\n      if (!this.resourceName || !this.resourceUrl) return;\n\n      const newResource = {\n        name: this.resourceName,\n        url: this.resourceUrl,\n        createdAt: new Date(),\n      };\n\n      await db\n        .collection('studyGroups')\n        .doc(this.$route.params.id)\n        .collection('resources')\n        .add(newResource);\n      this.resourceName = '';\n      this.resourceUrl = '';\n      this.fetchResources();\n    },\n    async fetchResources() {\n      const snapshot = await db\n        .collection('studyGroups')\n        .doc(this.$route.params.id)\n        .collection('resources')\n        .orderBy('createdAt')\n        .get();\n      this.resources = snapshot.docs.map((doc) => ({\n        id: doc.id,\n        ...doc.data(),\n      }));\n    },\n  },\n  created() {\n    this.fetchResources();\n  },\n};\n</script>\n\n<style scoped>\n.resource-share {\n  padding: 2em;\n}\n.resource {\n  margin-bottom: 1em;\n  padding: 1em;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n</style>\n"],"mappings":"AAwBA,SAASA,EAAC,QAAS,aAAa;AAEhC,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClB,IAAI,CAAC,IAAI,CAACJ,YAAW,IAAK,CAAC,IAAI,CAACC,WAAW,EAAE;MAE7C,MAAMI,WAAU,GAAI;QAClBP,IAAI,EAAE,IAAI,CAACE,YAAY;QACvBM,GAAG,EAAE,IAAI,CAACL,WAAW;QACrBM,SAAS,EAAE,IAAIC,IAAI,CAAC;MACtB,CAAC;MAED,MAAMX,EAAC,CACJY,UAAU,CAAC,aAAa,EACxBC,GAAG,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,EACzBJ,UAAU,CAAC,WAAW,EACtBK,GAAG,CAACT,WAAW,CAAC;MACnB,I