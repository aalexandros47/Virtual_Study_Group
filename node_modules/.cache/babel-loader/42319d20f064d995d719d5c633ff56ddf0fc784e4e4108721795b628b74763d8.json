{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  computed: {\n    authError() {\n      return this.$store.state.authError;\n    }\n  },\n  methods: {\n    submit() {\n      this.$store.dispatch('login', {\n        email: this.email,\n        password: this.password\n      }).then(() => {\n        if (!this.authError) {\n          this.$router.push('/create-study-group');\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","email","password","computed","authError","$store","state","methods","submit","dispatch","then","$router","push"],"sources":["/Users/ag47/virtual-study-group-platfor Design/src/views/LoginView.vue"],"sourcesContent":["<template>\n  <div\n    class=\"login-page min-h-screen flex items-center justify-center bg-gradient-to-r from-blue-500 to-purple-600\"\n  >\n    <div class=\"bg-white shadow-md rounded-lg p-8 m-4 max-w-sm w-full\">\n      <h2 class=\"text-2xl font-bold mb-4\">Login</h2>\n      <form @submit.prevent=\"submit\">\n        <div class=\"mb-4\">\n          <label for=\"email\" class=\"block text-gray-700\">Email:</label>\n          <input\n            type=\"email\"\n            id=\"email\"\n            v-model=\"email\"\n            required\n            class=\"w-full px-3 py-2 border rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-600\"\n          />\n        </div>\n        <div class=\"mb-4\">\n          <label for=\"password\" class=\"block text-gray-700\">Password:</label>\n          <input\n            type=\"password\"\n            id=\"password\"\n            v-model=\"password\"\n            required\n            class=\"w-full px-3 py-2 border rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-600\"\n          />\n        </div>\n        <button\n          type=\"submit\"\n          class=\"bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded-full transition duration-300\"\n        >\n          Login\n        </button>\n      </form>\n      <div v-if=\"authError\" class=\"auth-error mt-4 text-red-500\">\n        {{ authError }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'LoginView',\n  data() {\n    return {\n      email: '',\n      password: '',\n    };\n  },\n  computed: {\n    authError() {\n      return this.$store.state.authError;\n    },\n  },\n  methods: {\n    submit() {\n      this.$store\n        .dispatch('login', {\n          email: this.email,\n          password: this.password,\n        })\n        .then(() => {\n          if (!this.authError) {\n            this.$router.push('/create-study-group');\n          }\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.login-page {\n  background: linear-gradient(to right, #4a90e2, #50b7f5);\n}\n</style>\n"],"mappings":";AA0CA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,SAAS;IACpC;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAI,CAACH,MAAK,CACPI,QAAQ,CAAC,OAAO,EAAE;QACjBR,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACAQ,IAAI,CAAC,MAAM;QACV,IAAI,CAAC,IAAI,CAACN,SAAS,EAAE;UACnB,IAAI,CAACO,OAAO,CAACC,IAAI,CAAC,qBAAqB,CAAC;QAC1C;MACF,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}