ed: {\n    authMessage() {\n      return this.$store.state.authMessage;\n    },\n    authError() {\n      return this.$store.state.authError;\n    },\n  },\n  methods: {\n    submit() {\n      const studyGroup = {\n        name: this.name,\n        description: this.description,\n        duration: this.duration,\n      };\n      this.$store.dispatch('createStudyGroup', studyGroup);\n    },\n  },\n  created() {\n    this.$store.commit('setAuthMessage', null);\n    this.$store.commit('setAuthError', null);\n  },\n};\n</script>\n\n<style scoped>\n.create-study-group {\n  padding: 2em;\n}\n.auth-message {\n  color: green;\n  margin-top: 1em;\n}\n.auth-error {\n  color: red;\n  margin-top: 1em;\n}\n</style>\n"],"mappings":"AAwBA,eAAe;EACbA,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,WAAW,EAAE,EAAE;MACfC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,WAAW;IACtC,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,SAAS;IACpC;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,MAAMC,UAAS,GAAI;QACjBX,IAAI,EAAE,IAAI,CAACA,IAAI;QACfE,WAAW,EAAE,IAAI,CAACA,WAAW;QAC7BC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MACD,IAAI,CAACG,MAAM,CAACM,QAAQ,CAAC,kBAAkB,EAAED,UAAU,CAAC;IACtD;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,MAAM,CAACQ,MAAM,CAAC,gBAAgB,EAAE,IAAI,CAAC;IAC1C,IAAI,CAACR,MAAM,CAACQ,MAAM,CAAC,cAAc,EAAE,IAAI,CAAC;EAC1C;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}